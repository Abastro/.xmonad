<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <object class="GtkButton" id="window-item">
    <signal name="clicked" handler="window-item-activate" swapped="no"/>
    <style>
      <class name="window-item"/>
    </style>
    <child>
      <object class="GtkImage" id="window-item-icon">
        <property name="icon-size">3</property>
      </object>
    </child>
  </object>
</interface>