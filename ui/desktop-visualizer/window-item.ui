<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <template class="WindowItemView" parent="GtkBin">
    <child>
      <object class="GtkButton">
        <signal name="clicked" handler="activate" swapped="no"/>
        <child>
          <object class="GtkImage" id="window-item-icon">
            <property name="icon-size">3</property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>