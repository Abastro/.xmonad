<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <object class="GtkButton" id="battery">
    <property name="name">bat</property>
    <signal name="clicked" handler="battery-open" swapped="no"/>
    <child>
      <object class="GtkImage" id="battery-icon">
        <property name="icon-size">3</property>
      </object>
    </child>
  </object>
</interface>