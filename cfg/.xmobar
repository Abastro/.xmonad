Config{
    font = "xft:Noto Sans:pixlsize=20:bold,Font Awesome 5 Free:style=Solid:pixelsize=20"
  , bgColor = "black"
  , fgColor = "grey"
  , position = TopW L 90
  , lowerOnStart = True
  , commands = [
    Run Cpu ["-t", "\xf2db <total>%", "-p", "2", "-c", "0"
      , "-L", "10", "-H", "50", "-n", "#cccc77", "-h", "yellow"] 10
    , Run Memory ["-t", "\xf538 <usedratio>%", "-p", "2", "-c", "0"] 10
    , Run Date "%a, %b %d %H:%M" "date" 10
    , Run Battery ["-t", "\xf5df <left>%<acstatus>", "-p", "3"
      , "--", "-O", "<fc=green>+</fc>", "-i", "", "-o", "<fc=red>-</fc>"] 10
    , Run StdinReader
    ]
  , sepChar = "%"
  , alignSep = "}{"
  , template = " %StdinReader% } <fc=#ee9a00>%date%</fc> { %battery% | %cpu% | %memory% "
  }
